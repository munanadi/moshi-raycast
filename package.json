{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mochi-raycast",
  "title": "mochi-raycast",
  "description": "Raycast for Mochi",
  "icon": "command-icon.png",
  "author": "munanadi",
  "license": "MIT",
  "commands": [
    {
      "name": "market-data",
      "title": "Market Data",
      "description": "Shows the Market data",
      "mode": "view"
    },
    {
      "name": "coin-gecko-tokens",
      "title": "CG Tokens",
      "description": "List of all Coin Gecko Tokens",
      "mode": "view"
    },
    {
      "name": "watchlist",
      "title": "Watchlist",
      "description": "Shows Marketdata at a glance",
      "mode": "view"
    },
    {
      "name": "heatmap",
      "title": "Heatmap",
      "description": "Shows Market as a Heatmap",
      "mode": "view"
    },
    {
      "name": "tokens-list",
      "title": "Tokens",
      "description": "Lists all tokens",
      "mode": "view"
    },
    {
      "name": "trending",
      "title": "Trending",
      "description": "Shows information of trending tokens",
      "mode": "view"
    },
    {
      "name": "ticker",
      "title": "Ticker",
      "description": "Shows information about a ticker",
      "mode": "view",
      "arguments": [
        {
          "name": "base",
          "placeholder": "Symbol/Base_Target",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "convert",
      "title": "Convert",
      "description": "Convert between two different tokens",
      "mode": "view",
      "arguments": [
        {
          "name": "amount",
          "placeholder": "2.0",
          "type": "text",
          "required": true
        },
        {
          "name": "from",
          "placeholder": "SOL",
          "type": "text",
          "required": true
        },
        {
          "name": "to",
          "placeholder": "ETH",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.5.2",
    "axios": "^1.3.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
